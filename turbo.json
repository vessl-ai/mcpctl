{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "lint": {
      "dependsOn": [
        "build"
      ]
    },
    "build": {
      "dependsOn": [
        "check-types"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "dependsOn": [
        "check-types"
      ],
      "persistent": true,
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}